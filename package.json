{
    "name": "bro-ai",
    "version": "1.0.0",
    "main": "dist/index.js",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
    },
    "dependencies": {
        "axios": "^1.8.4",
        "dotenv": "^16.5.0",
        "telegraf": "^4.16.3"
    },
    "devDependencies": {
        "@types/node": "^22.14.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.1",
        "nodemon": "^3.1.9",
        "prettier": "3.5.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
